@model FootballScoutApp.Models.Message

<h2>Message Details</h2>

<div class="form-group">
    <label>From:</label>
    <p>@Model.Sender.Email</p>
</div>
<div class="form-group">
    <label>Received:</label>
    <p>@Model.Timestamp</p>
</div>
<div class="form-group">
    <label>Message:</label>
    <p>@Model.Content</p>
</div>

<a href="@Url.Action("Inbox")" class="btn btn-primary">Back to Inbox</a>
